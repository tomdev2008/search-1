@charset "utf-8";
/* CSS Document */
ul,ol{ list-style:none outside none}
/*a{color:#0063DC; text-decoration:none;outline:none}
a:hover{color:#f60; text-decoration:underline}*/
/*.h{color:#ff5500 !important;}
.loading-16{}
.loading-32{}*/
.hl{color:#ff5500 !important;}
.clearfix:after{clear:both;content:".";display:block;height:0;visibility:hidden;_height:1%;}
.fl{ float:left;}
.fr{ float:right}
.hide{ display:none;}
.cb{ clear:both}
.cl{ clear:left}
.cr{ clear:right}
.tl{text-align: left;}
.tr {text-align: right;}
.tc {text-align: center;}
.vm {vertical-align: middle;}
.pr {position: relative;}
.pa {position: absolute;}
.fn {font-weight: normal;}
.fbo{width:130px;overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
.fb {font-weight: bold;}
.tlt{ text-decoration:line-through}
.zoom {zoom:1}

/******颜色******/
.cred{ color:#F00}
.cgreen{ color:#090}
/******间距******/
.p5{ padding:5px}
.pt5{ padding-top:5px}
.pl5{ padding-left:5px}
.pr5{ padding-right:5px}
.pb5{ padding-bottom:5px;}
.p10{ padding:10px}
.pt10{ padding-top:10px;}
.pl10{ padding-left:10px;}
.pr10{ padding-right:10px}
.pb10{ padding-bottom:10px;}
.mt5{ margin-top:5px;}
.mr5{ margin-right:5px;}
.mb5{ margin-bottom:5px;}
.ml5{ margin-left:5px;}
.mt10{ margin-top:10px;}
.mr10{ margin-right:10px;}
.mb10{ margin-bottom:10px;}
.ml10{ margin-left:10px;}
.pl3{ padding-left:3px}
.pr3{ padding-right:3px}

.wp100{ width:100%}

/*搜索小图标*/
.xs-ico,
.xs-crumb .icarr,
.facet-select-all .close-ico,
.facet-line .more-opt,
.xs-sort-bar .sort-box dd i,
.xs-toolbar .showmode  b,
.xs-toolbar .l_page,
.xs-toolbar .r_page,
.xs-itemsbox .showprice,
.xs-itemsbox .delprice,
.xs-catalog .bd dt b,
.xs-saleitemslist .price,
.xs-pagebar a.pre-page,
.xs-pagebar a.next-page,
.xs-catalog-search i
{ background:url(../image/xsearchico20120615.png?__=20120803) no-repeat 999em 999em;}

/*搜索背景*/
.xs-bg,
.xs-crumb .nav i,
.xs-facetbox .facet-hd .tit,
.xs-sort-bar .sort-box dd b,
.xs-sort-bar .sort-box dd a,
.xs-toolbar,
.xs-toolbar .pricerange .btn,
.xs-itemsbox .onsale-btn,
.xs-itemsbox .offsale-btn,
.Pageopt .page-btn,
.xs-catalog .hd,
.xs-catalog-search .hd,
.bfd-click-link,
.xs-btm-search .searchbox .sbtn,
.xs-nrbox .xs-nrico,
.xs-mkttypebox .tabs a,.xs-mkttypebox .tabs b,.xs-mkttypebox .tabs-arr
{ background:url(../image/xsearch20120615.png?__=20120803) no-repeat 999em 999em;}

/*搜索500结果页面*/
.xs-main500 .xs-nrmain{background:url(../image/500_pic_search.jpg) no-repeat 0 0}

/*广告位*/
.xs-banner-topmid{ margin:10px auto 0; width:950px; line-height:0;}
.xs-banner-topmid img{ border:0 none; margin:0; padding:0}

/*骨架*/
.xs-mainsection{ width:950px; margin:10px auto; font-size:12px;zoom:1; line-height:1.5em; color:#404040}
.xs-mainsection button{ font-size:12px;}
.xs-section{ float:left; width:100%;zoom:1}
.xs-mainsection a:hover{text-decoration:underline; color:#f60}
.xs-main{ margin-left:215px;zoom:1}
.xs-aside{ width:205px; float:left; margin-left:-100%;_margin-left:-950px;zoom:1; display:inline}
/*面包屑*/
.xs-crumb{ padding-left:10px; height:25px; line-height:25px; border-bottom:1px solid #E5E5E5; position:relative;zoom:1}
.xs-crumb .hd{ float:left;}
.xs-crumb .nav{ float:left}
.xs-crumb .nav li{ float:left;padding-left:20px; position:relative}
.xs-crumb .nav li.first{ padding-left:0}
.xs-crumb .nav li span{float:left;position:relative;}
.xs-crumb .nav i{ display:block; width:6px; height:11px; position:absolute; left:7px; top:50%; margin-top:-5px; background-position:-201px -95px;  line-height:0; font-size:0; font-family:Arial;}
.xs-crumb .icarr{ display:block; width:6px; height:7px; background-position:0 0; position:absolute; top:50%; margin-top:-3px;left:0px; line-height:0; font-size:0; font-family:Arial;}
.xs-crumb .totalcount{ position:absolute; top:0; right:0;}

/*相关搜索*/
.xs-relatetermbox{position:relative;border:1px solid #ededed;height:30px;overflow:hidden;background-repeat: repeat-x; background-position:0 -135px;zoom:1; margin-top:10px;} 
.xs-relatetermbox .hd{position:absolute;top:0;left:0;height:30px;line-height:30px;display:block;padding-left:10px;color:#3E3E3E}
.xs-relatetermbox .bd{margin:8px 10px 0 70px;overflow:hidden;zoom:1}
.xs-relatetermbox .bd a{float:left;margin-bottom:20px;display:inline;white-space:nowrap;padding:0px 10px;border-left:1px dotted #ccc;line-height:14px;height:14px;color:#005AA0}
.xs-relatetermbox .bd a:hover{color:#f60}
.xs-relatetermbox .bd a.first{border-left:0 none;}

/*搜索无结果*/
.xs-errortermbox{position:relative;border:1px solid #ECD28B;background-color:#FFFEEE;margin-top:10px;overflow:hidden;zoom:1}
/*.errmsg{color:#FF5500;margin:0 5px;}
.leon-err-hl-a{color:#0063DC;margin:0 5px;}*/
.errmsg{font-size:14px;font-weight:bold;color:#666666;margin:12px;overflow:hidden;line-height:16px;}
.errmsg .hl{color:#FF5500;margin:0 5px;}

/*ebay对接的tab切换*/
.xs-mkttypebox{ position:relative; height:29px; margin-top:10px; border-bottom:2px solid #CC0010}
.xs-mkttypebox .tabs{}
.xs-mkttypebox .tabs-arr{ display:none; width:9px; height:5px; line-height:0; font-size:0; background-position:-174px -90px; position:absolute; bottom:-7px; left:50%; margin-left:-4px;}
.xs-mkttypebox .tabs li{float:left;height:29px;line-height:29px;margin-right:5px;position:relative}
.xs-mkttypebox .tabs a{float:left;margin-top:2px; height:27px; line-height:27px; _line-height:29px;zoom:1; overflow:hidden; padding:0 13px; background-position:right -340px; position:relative;zoom:1; }
.xs-mkttypebox .tabs b{position:absolute; display:block; width:5px; height:27px; background-position:left -340px; top:0; left:0; line-height:0; font-size:0}
.xs-mkttypebox .tabs li span{font-size:14px}
.xs-mkttypebox .tabs li .itemcount{font-family:'宋体'}
.xs-mkttypebox .tabs li.active a{ height:29px; margin-top:0; line-height:29px;_line-height:31px;background-position:right -310px; color:#fff; padding:0 20px;}
.xs-mkttypebox .tabs li.active b{ height:29px;background-position:left -310px;}
.xs-mkttypebox .tabs li.active .tabtit{ font-weight:bold}
.xs-mkttypebox .tabs a:hover{ text-decoration:none}
.xs-mkttypebox li.active .tabs-arr{ display:block}
.xs-mkttypebox li.disable a{ color:#878787; cursor:default}

/*筛选区*/
.xs-facetbox{ margin-top:10px;}
.xs-facetbox .facet-hd{ height:22px;}
.xs-facetbox .facet-hd .tit{ color:#fff; float:left; height:22px; width:87px; line-height:22px; background-position:0 -90px; text-align:center}
.facet-hd{background-position:-66px -60px; width:87px; height:20px; color:#fff; text-align:center; font-weight:normal; font-size:12px; line-height:20px;}
.facet-bd{ background-color:#F7F7F7; border:1px solid #DFDFDF; padding:5px 10px 0;}
.facet-bd a:hover{ text-decoration:none}
.facet-select-all .normal-item-box{ padding-left:0; padding-right:0;}
.facet-select-all .close-ico{ position:absolute; cursor:pointer;display:block; width:12px; border:1px solid #939192; height:12px; top:50%; margin-top:-7px; right:3px; background-color:#fff; background-position:2px -317px;overflow:hidden; line-height:0; font-size:0;}
.facet-select-all .normal-item-box a{ display:block; position:relative; padding-right:25px; background-color:#939192; color:#fff;zoom:1; }
.facet-select-all .normal-item-box a:hover .close-ico{border-color:#f60;background-position:2px -337px}
.facet-select-all .normal-item-box a:hover .close-ico{_border-color:#fff;_background-position:2px -317px}
.facet-line{ position:relative; padding-left:60px; zoom:1; border-top:1px dotted #ccc}
.xs-facetbox .nodot{ border-top:0 none}
.facet-line .line-hd{ position:absolute; width:65px; height:20px; line-height:20px; top:10px; left:0; }
.facet-line .line-bd{zoom:1;padding-top:5px;}
.brand-facet-box .line-bd{ margin-bottom:2px}
.brand-btn-box{ background-color:#E7E7E7; height:30px; padding:0 5px; position:relative;zoom:1}
.brand-btn-box li{ float:left; padding-top:5px; margin-right:10px;}
.brand-btn-box li a{ float:left;padding-top:8px;padding:0 8px;height:20px; line-height:20px;}
.brand-btn-box li a:hover,.facet-line .brand-btn-box li a.active{ color:#fff; background-color:#939192;}
.facet-line .more-opt{ height:20px;line-height:20px; width:26px; padding-left:16px; background-position:0px -15px;display:block; position:absolute; top:4px; right:10px; display:block; overflow:hidden;}
.facet-line .more-opt:hover{background-position:0px -35px;}
.facet-line .down{background-position:0px -55px;}
.facet-line .down:hover{background-position:0px -75px;}
.brand-btn-box .more-opt{top:4px;}
.brand-item-box{ height:0; overflow-x:hidden; overflow-y:hidden; overflow:hidden;_height:0;_zoom:1;}
.brand-item-box ul{ padding:0px 5px;*zoom:1;overflow:hidden}
.brand-item-box li{ float:left; width:110px; height:20px; line-height:20px; overflow:hidden; padding:5px 10px 7px 0;*zoom:1;}
.brand-item-box li span{color:#999}
.brand-item-box li a{ padding:0 8px; width:94px;height:20px; line-height:20px; float:left;overflow:hidden; white-space:nowrap;text-overflow:ellipsis;}
.brand-item-box li a:hover,.brand-item-box li a:hover span{ background-color:#939192;color:#fff;}
.normal-item-box{ position:relative;*zoom:1; padding:5px 35px 0 0px;}
.normal-item-box ul{*zoom:1;overflow:hidden}
.normal-item-box .all-btn{ text-align:center; display:block; position:absolute; top:5px; left:5px; width:40px; height:20px; line-height:20px;}
.normal-item-box li{ float:left; height:20px; line-height:20px; padding:0 10px 10px 0;*zoom:1;}
.normal-item-box li a{padding:0 8px;height:20px; line-height:20px; float:left; display:block;*zoom:1;white-space:nowrap;}
.normal-item-box li a:hover,.normal-item-box .all-btn:hover,.normal-item-box .active{background-color:#939192;color:#fff;}
.normal-item-box li a:hover span{color:#fff}
.facet-item-box{overflow:hidden;zoom:1; min-height:30px;height:30px;}
.facet-item-box ul{ overflow:hidden}
.normal-item-box li span{color:#999}
.normal-item-box .hide{ display:none}

/*排序功能*/
.xs-filterbox{ margin-top:10px}
/*.xs-sort-bar .sort-box dd a,.xs-sort-bar .sort-box dd b,.xs-sort-bar .sort-box dd i,.facet-select-all .close-ico{ background:url(img/search-sort-bar-01.png) no-repeat 999em 999em}*/
.xs-sort-bar{ height:26px;}
.xs-sort-bar .sort-box{line-height:26px;line-height:28px \9; height:26px; float:left; overflow:hidden;zoom:1}
.xs-sort-bar .sort-box dt{ float:left; width:58px;}
.xs-sort-bar .sort-box dt label{ padding-left:11px;}
.xs-sort-bar .sort-box dd{ float:left; margin-right:3px;}
.xs-sort-bar .sort-box dd a{ float:left; padding:0 20px 0 13px; height:26px; overflow:hidden;position:relative;zoom:1; background-position:right -26px; font-size:14px;}
.xs-sort-bar .sort-box dd a.w2{ width:30px}
.xs-sort-bar .sort-box dd a.nonearr{padding-left:16px;padding-right:16px;}
.xs-sort-bar .sort-box dd a.w4{ width:60px}
.xs-sort-bar .sort-box dd b{ display:block; top:0; left:0; height:26px; width:5px; line-height:0; background-position: 0 -26px; position:absolute}
.xs-sort-bar .sort-box dd i{ display:block; top:50%; margin-top:-3px;_margin-top:-4px; right:8px; line-height:0; position:absolute; width:7px; height:8px; background-position:0 -240px;}
.xs-sort-bar .sort-box dd i.down{ background-position: 0 -260px}
.xs-sort-bar .sort-box a:hover,.xs-sort-bar .sort-box a.active{background-position:right 0; text-decoration:none; color:#fff; font-weight:bold}
.xs-sort-bar .sort-box a:hover b,.xs-sort-bar .sort-box a.active b{background-position:0 0;}
.xs-sort-bar .sort-box a:hover i,.xs-sort-bar .sort-box a.active i{background-position: 0 -280px}
.xs-sort-bar .sort-box a:hover i.down,.xs-sort-bar .sort-box a.active i.down{ background-position:0 -300px;}
.xs-sort-bar .buyable-box{ float:right; line-height:26px;}
.xs-sort-bar .buyable-box label{ float:left; margin-left:10px}
.xs-sort-bar .buyable-box input{ vertical-align:middle; margin-right:5px;}

.xs-toolbar{ border-width:2px 1px 1px; border-color:#505050 #D7D7D7 #D7D7D7; border-style:solid; height:37px; background-position:0 -135px; background-repeat:repeat-x}
.xs-toolbar  .showmode{ float:left; padding:8px 0 0 10px; }
.xs-toolbar  .showmode a{ float:left; position:relative; border:1px solid #a3a3a3; background-color:#fff; height:20px; line-height:18px;line-height:20px\9; padding-left:22px; padding-right:5px}
.xs-toolbar  a:hover{ text-decoration:none;}
.xs-toolbar  .showmode a:hover{ border-color:#f60; color:#5B5B5B}
.xs-toolbar  .showmode a b{display:block; width:11px; height:11px; line-height:0; font-size:0; background-position:0 -140px; position:absolute; top:50%; margin-top:-6px; left:5px}
.xs-toolbar  .showmode a:hover b{ background-position:0 -120px}
.xs-toolbar  .showmode a b.listmode{background-position:0 -180px; }
.xs-toolbar  .showmode a:hover b.listmode{background-position:0 -160px; }
.xs-toolbar .pricerange{ float:left; padding:5px 8px; margin:3px 0 0 20px; height:21px}
.xs-toolbar .pricerange .txt-input{height:20px; width:35px; border:1px solid #b3b3b3; text-align:center; vertical-align:middle;}
.xs-toolbar .pricerange .cancel{display:none}
.xs-toolbar .pricerange-active{ background-color:#EFEFEF;}
.xs-toolbar .pricerange-active .cancel{display:inline}
.xs-toolbar .pricerange .btn{ width:44px; height:22px; background-position:-125px -90px; line-height:20px; border:0 none; cursor:pointer; padding:0; font-family:Tahoma; vertical-align:middle}
.xs-toolbar .xs-minpage{ float:right; padding:7px 10px 0 0; line-height:22px;}
.xs-minpage li{ float:left; margin-left:10px;}
.xs-minpage .l_page,.xs-toolbar .r_page{ float:left; position:relative; line-height:22px;line-height:25px\9; height:22px;zoom:1; overflow:hidden;}
.xs-minpage .l_page{ padding-left:10px; background-position:0 -432px}
.xs-minpage .r_page{ padding-right:10px; background-position:40px -452px}
.xs-minpage .l_page:hover{ background-position:0 -392px}
.xs-minpage .r_page:hover{background-position:40px -412px}
.xs-minpage .disable a{ color:#ccc !important; cursor:default !important}
.xs-minpage .disable a.l_page,.xs-toolbar .disable a.l_page:hover{background-position:0 -472px}
.xs-minpage .disable a.r_page,.xs-toolbar .disable a.r_page:hover{background-position:40px -492px}

/*商品列表部分-大图模式*/
.xs-itemsbox{}
.xs-itemsul-img{ padding:10px 0 0 10px;*padding-bottom:10px}
.xs-itemsul-img .item{ float:left; width:240px; height:335px; border-bottom:1px solid #EFEFEF; margin-bottom:15px}
.xs-itemsul-img .item ol{ padding-left:2px;}
.xs-itemsul-img .pic{}
.xs-itemsul-img .pic a{display:block; width:220px; height:220px; padding:2px; border:1px solid #fff; position:relative;*zoom:1}
.xs-itemsul-img .pic a:hover{ border-color:#f90}
.xs-itemsul-img .pic img{ width:220px; height:220px; border:0 none;}
.xs-itemsul-img .tit{ height:36px; overflow:hidden;line-height:18px; width:220px; margin-top:5px;}
.xs-itemsul-img .tit a{ overflow:hidden; display:block}
.xs-itemsul-img .price{ overflow:hidden; margin-top:5px; margin-bottom:0;}
.xs-itemsul-img .btnbar{ margin-top:5px;}
.xs-itemsul-img .onsale-btn,.xs-itemsul-img .offsale-btn{ }
.xs-itemsbox .showprice{ height:20px; line-height:20px; font-size:18px; font-weight:bold; padding-left:13px; color:#D80C18; background-position:2px -355px}
.xs-itemsbox .delprice{ height:20px; line-height:20px; font-size:14px; color:#a2a2a2; font-weight:bold; font-family:'宋体'; margin-left:5px; padding-left:10px; background-position:0 -378px; text-decoration:line-through}
.xs-itemsbox .onsale-btn,.xs-itemsbox .offsale-btn{width:62px;height:21px;display:block;float:left;line-height:20px;line-height:21px\9;text-align:center;}
.xs-itemsbox .onsale-btn{ background-position:0 -210px}
.xs-itemsbox .offsale-btn{ background-position:-62px -210px;color:#FF6400}
.xs-itemsbox .onsale-btn:hover{ text-decoration:none; color:#5B5B5B}
.xs-itemsbox .offsale-btn:hover{ text-decoration:none; color:#e35e03}
.xs-itemsbox .saleout-ico{display:block;position:absolute;z-index:5; bottom:0; right:0; width:94px; height:76px; background:url(../image/search-sale-out.png?__=20120619) no-repeat 0 0;
_background:url(../image/search-sale-out-p8.png?__=20120619) no-repeat 0 0;_filter:alpha(opacity=90);}
/*商品列表部分-列表模式*/
.xs-itemsul-detail{padding-left:10px; padding-right:10px; padding-top:20px;}
.xs-itemsul-detail .item{zoom:1;border-bottom:1px solid #efefef;height:125px; margin-bottom:15px;}
.xs-itemsul-detail .pic{float:left;width:125px;}
.xs-itemsul-detail .pic a{ width:100px; height:100px; padding:2px; border:1px solid #DFDFDF; display:block; position:relative;*zoom:1}
.xs-itemsul-detail .pic a:hover{ border-color:#f90}
.xs-itemsul-detail .pic img{width:100px;height:100px;}
.xs-itemsul-detail .tit{float:left;width:315px;height:80px; font-size:14px; color:#2c2c2c; line-height:1.5em; padding-top:10px; overflow:hidden; }
.xs-itemsul-detail .price{float:left;width:145px; text-align:center}
.xs-itemsul-detail .btnbar{float:left; padding-left:15px; padding-top:15px}





/* 下翻页 */
.xs-pagebar{ margin-top:10px;}
.xs-pagebar .Pagecon{ float:right;}
.Pagecon .Pagenum{ float:left; height:24px;}
.Pagecon .Pagenum a,.xs-pagebar .Pagenum span.curpage{ float:left; display:inline;zoom:1; border:1px solid #B8B8B8; height:22px; line-height:22px; padding:0 8px; font-size:12px; margin:0 3px}
.Pagecon .Pagenum a:hover{ border-color:#f60; color:#f60; text-decoration:none}
.Pagecon .Pagenum a.pre-page{ padding-left:15px; background-position:5px -433px}
.Pagecon .Pagenum a.next-page{ padding-right:15px;background-position:48px -453px}
.Pagecon .Pagenum a.pre-page:hover{background-position:5px -393px}
.Pagecon .Pagenum a.next-page:hover{background-position:48px -413px}
.Pagecon .Pagenum a.pre-disable,.xs-pagebar .Pagenum a.pre-disable:hover{background-position:5px -473px !important; color:#ccc; border-color:#ccc; cursor:default}
.Pagecon .Pagenum a.next-disable,.xs-pagebar .Pagenum a.next-disable:hover{background-position:48px -493px !important;color:#ccc; border-color:#ccc; cursor:default}
.Pagecon .Pagenum span.curpage{ border-color:#f60; font-weight:bold; color:#f60; cursor:default}
.Pagecon .Pagenum .elli{ cursor:default; float:left; border:0 none; height:24px; line-height:24px;padding:0 px; font-size:12px; margin:0 3px}
.xs-pagebar .Pageopt{ float:left; line-height:22px; height:24px; margin-left:10px}
.xs-pagebar .Pageopt .totalnum{ float:left; height:24px;}
.xs-pagebar .Pageopt .gotopage{ float:left; margin-left:10px; height:24px; line-height:22px;}
.xs-pagebar .Pageopt .txt-input{ border:1px solid #B8B8B8; height:18px; width:33px; text-align:center; vertical-align:middle; margin:0 5px}
.xs-pagebar .Pageopt .page-btn{ display:inline; border:0 none; padding:0; line-height:18px; height:22px; width:44px; background-position:-125px -90px; vertical-align:middle; margin-left:5px; cursor:pointer}


/*左侧分类树*/
.xs-catalog{}
.xs-catalog .hd{ width:205px; height:38px; line-height:38px; background-position:0 -52px;}
.xs-catalog .hd .tit{ font-size:14px; font-weight:bold; line-height:38px; padding:0 0 0 15px; color:#fff;}
.xs-catalog .hd .tit a{ color:#fff}
.xs-catalog .bd{ border:1px solid #d7d7d7; border-top:0 none}
.xs-catalog .bd dt{ background-color:#eee; color:#555; height:24px; line-height:24px; padding-left:15px; position:relative; border-top:1px solid #d7d7d7}
.xs-catalog .bd dt b{ display:block; width:9px; height:5px; position:absolute; top:50%; margin-top:-3px; right:10px;}
.xs-catalog .bd dt.down b{ background-position:0 -220px}
.xs-catalog .bd dt.down{cursor:pointer;}
.xs-catalog .bd dt.up b{ background-position:0 -200px}
.xs-catalog .bd dt a{ font-weight:bold; font-size:12px; color:#555}
.xs-catalog .bd dt a:hover{ color:#f60;}
.xs-catalog .bd dd{ }
.xs-catalog .bd .childlist{padding:10px 10px 10px 15px}
.xs-catalog .bd .childlist li{ height:24px; line-height:24px;}
.xs-catalog .bd .childlist .num{ color:#999; padding-left:5px;}
.xs-catalog  dd a.active {color: #c60;font-weight: bold;}

/*左侧分类树-搜索页面样式*/
.xs-catalog-search{white-space:nowrap}
.xs-catalog-search .hd{ width:205px; height:38px; line-height:38px; background-position:0 -52px;}
.xs-catalog-search .hd .tit{ font-size:14px; font-weight:bold; line-height:38px; padding:0 0 0 15px; color:#fff;}
.xs-catalog-search .bd{border:1px solid #d7d7d7; border-top:0 none; padding:10px 0 10px}
.xs-catalog-search dl.ct-dl{ margin-bottom:5px; padding-left:10px}
.xs-catalog-search .all-mode dt{ font-weight:bold; }
.xs-catalog-search dl{ margin-bottom:5px;}
.xs-catalog-search dl b{ font-size:10px  !important; font-weight:normal  !important; color:#999 !important; padding-left:3px;}
.xs-catalog-search dt{ padding-left:15px; position:relative; display:block;margin-bottom:2px; height:20px; line-height:20px; overflow:hidden}
.xs-catalog-search dt a{ color:#000}
.xs-catalog-search dt a:hover,.xs-catalog-search dd a:hover{color:#c60}
.xs-catalog-search .child-ct{margin-bottom:2px;}
.xs-catalog-search .child-ct dt{ font-weight:normal; }
.xs-catalog-search .child-ct dt.fb{font-weight:bold}
.xs-catalog-search .child-ct a{ color:#3e3e3e}
.xs-catalog-search dd{ padding-left:13px;}
.xs-catalog-search .plus,.xs-catalog-search .minus,.xs-catalog-search .cur{ cursor:pointer; display:block; width:11px; height:11px; position:absolute; top:50%; margin-top:-5px; left:0px; background-position:0 -520px}
.xs-catalog-search .minus{ background-position:0 -540px}
.xs-catalog-search .return-all{ height:20px; line-height:20px; position:relative; padding-left:5px; margin-bottom:5px;}
.xs-catalog-search .return-all a{ padding-left:10px;position:relative;}
.xs-catalog-search .return-all a:hover{color:#c60;}
.xs-catalog-search i.ret{cursor:pointer; position:absolute; display:block; width:5px; height:9px; top:50%; margin-top:-4px; left:0;background-position:0 -100px}
.xs-catalog-search  dl i.ret{ left:3px; cursor:default}
.xs-catalog-search i.cur{ cursor:default; background-position:0 -560px}


/* 百分点推荐的销售排行 */
.xs-saleitemslist{ margin-top:10px; border:1px solid #DFDFDF}
.xs-saleitemslist .hd{ background-color:#F4F4F4; color:#000; font-family:'微软雅黑'; font-size:15px; height:30px; line-height:30px;}
.xs-saleitemslist .hd .tit{ padding-left:15px;}
.xs-saleitemslist .hd .tag{ color:#B1B0B0; margin-left:5px;}
.xs-saleitemslist .bd{ position:relative}
.xs-saleitemslist ul{ padding:10px 8px 8px 8px;}
.xs-saleitemslist li{ padding-left:90px; position:relative; height:80px; margin-bottom:8px; border-bottom:1px dashed #efefef; padding-bottom:8px;}
.xs-saleitemslist li.nodot{ border-bottom:0 none}
.xs-saleitemslist li a.img{position:absolute; display:block;top:0px; left:0px;width:80px; height:80px;}
.xs-saleitemslist li a.img img{width:80px; height:80px;}
.xs-saleitemslist li a.tit{height:36px;overflow:hidden;display:block;line-height:18px;word-break:break-all;word-wrap:break-word;}
.xs-saleitemslist .bfd-click-link{ display:block; position:absolute; bottom:5px; right:5px; line-height:0; font-size:0; background-position:0 -115px; width:97px; height:13px;text-indent:-999em}
.xs-saleitemslist .price{ height:20px; padding-left:12px; margin-top:5px; background-position:0 -357px; line-height:20px; font-size:14px; font-weight:bold; color:#D80C18 !important}

/* 百分点推荐底部官方推荐 */
.xs-salexiurecom-bottombox{ background-color:#F6F6F6; width:950px; margin:20px auto 0}
.xs-xiurecombox a:hover{ text-decoration:underline}
.xs-xiurecombox .hd{ padding:0 15px}
.xs-xiurecombox .hd .tit{ height:34px; line-height:34px; border-bottom:1px solid #d7d7d7; font-size:14px; color:#000; font-family:'微软雅黑'}
.xs-xiurecombox .bd{ padding:0 15px;position:relative}
.xs-xiurecombox .bd ul{ border-top:1px solid #fff; padding-top:10px; padding-bottom:10px; overflow:hidden}
.xs-xiurecombox .bd li{ float:left; width:160px; padding:7px 10px; border-left:1px dashed #f9f9f9; border-right:1px dashed #e9e9e9; overflow:hidden}
.xs-xiurecombox .bd li:hover{background-color:#fdfcea}
.xs-xiurecombox .bd li.first{ border-left:0 none}
.xs-xiurecombox .bd li.last{ border-right:0 none}
.xs-xiurecombox .bd .picbox{ text-align:center;}
.xs-xiurecombox .bd .picbox img{ width:100px; height:100px;}
.xs-xiurecombox .bd .tit{ margin-top:5px; height:36px; line-height:18px; overflow:hidden;}
.xs-xiurecombox .bd .price{ color:#D80C18 !important; font-size:18px; height:20px; line-height:20px; margin-top:5px; background-position:0 -356px; padding-left:12px}
.xs-xiurecombox .bfd-click-link{ position:absolute; bottom:8px; right:10px;display:block; line-height:0; font-size:0; background-position:0 -115px; width:97px; height:13px;text-indent:-999em}

/* 底部搜索 */
.xs-btm-search{ height:90px; background-color:#F6F6F6; width:950px; margin:20px auto 0; overflow:hidden}
.xs-btm-search .searchbox{ width:560px; height:28px; margin:30px auto 0;}
.xs-btm-search .searchbox .inputbg{ width:480px; border:1px solid #989898; border-right:0 none; height:28px; overflow:hidden; float:left; border-radius:2px 0 0 2px}
.xs-btm-search .searchbox .txt-input{ width:480px; height:28px; line-height:28px; border:0 none;}
.xs-btm-search .searchbox .sbtn{ background-position:-125px -210px; float:left; width:77px; height:30px; border:0 none; cursor:pointer; border-radius:0 2px 2px 0}


/* 搜索首页样式 */
.xs-indexhead{ width:950px; margin:0 auto;}
.xs-indexhead .logo{ width:213px; height:52px; margin:90px auto 0; overflow:hidden}
.xs-indexhead .logo img{width:213px; height:52px;}
.xs-indexsearchbox{ width:950px; margin:0 auto;}
.xs-indexsearchbox .xs-btm-search{ background-color:#fff; margin-top:30px;}
.xs-indexsearchbox .xs-btm-search .welcome-txt{ height:30px; line-height:30px; position:absolute; top:0; left:7px; font-size:14px; color:#aaa; }
.xs-indexsearchbox .xs-btm-search .searchbox{ height:30px;width:590px; position:relative}
.xs-indexsearchbox .xs-btm-search .searchbox .inputbg{ height:30px; padding-left:5px;}
.xs-indexsearchbox .xs-btm-search .searchbox .txt-input{ height:30px; line-height:30px; font-size:14px}
.xs-indexsearchbox .xs-btm-search .searchbox .sbtn{ background-position:-125px -240px; height:32px; width:100px}
.xs-indexfoot{ width:950px; margin:0 auto;padding-bottom:30px}
.xs-indexfoot .copyright{margin:120px auto 0; text-align:center; width:430px; border-top:1px solid #f7f7f7; padding-top:10px;}


/*搜索无结果页面*/
.xs-nrmain{ /*width:735px; float:left*/ margin-top:20px;}
.xs-nraside{ width:205px; float:left; margin-left:10px;}
.xs-nrbox{position:relative;zoom:1;color:#404040; border:1px solid #E5E5E5;}
.xs-nrbox .xs-nrico{position:absolute;top:25px;left:110px;width:82px;height:73px; background-position:0 -235px;}
.xs-nrbox .sorry-box{ padding:25px 0 25px 200px;}
.xs-nrbox .sorry-box dt{margin:0 0 25px 0;font-size:14px;font-weight:bold;color:#333;}
.xs-nrbox .sorry-box dd{margin:0 0 4px 0;}
.xs-nrbox .sorry-box .hl{margin:0 5px; color:#f60}
.xs-nrbox .ulike-box{padding:20px 0 35px 110px; border-top:1px solid #E5E5E5;/* background-color:#FFF8F6;*/ background-color:#FFFEEE;zoom:1}
.xs-nrbox .ulike-box dt{margin-bottom:10px; overflow:hidden; clear:both;}
.xs-nrbox .ulike-box dd{ margin-right:10px; padding-right:10px;float:left;border-right:1px solid #ccc; height:12px; line-height:10px;}
.xs-nrbox .ulike-box a{ color:#005AA0}
.xs-nrbox .ulike-box a:hover{color:#f90}
.xs-nr-xiurecombox{ margin-top:20px; background-color:#F6F6F6;}
.xs-nr-xiurecombox .bd li{ border-right:0 none; border-left:0 none; margin-bottom:15px}
.xs-nr-xiurecombox .bd li.nobtmborder{ border-bottom:0 none}

/*500页面*/
.xs-main500{margin-top:0px;}
.xs-main500 .xs-nrmain{margin-top:0; position:relative; height:620px;}
.xs-main500 ul.note{ position:absolute; top:220px; left:515px; font-size:14px; font-family:'微软雅黑' !important; color:#000; line-height:1.5em;}
.xs-main500 ul.note li{ margin-bottom:5px;}
.xs-main500 ul.note li a.refresh{ color:#c91300; padding:0 3px}
.xs-main500 ul.note .callus{ color:#c91300; margin-top:20px;}
.xs-main500 ul.note .callus span{ font-size:22px; font-weight:bold}


