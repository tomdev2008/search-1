.ssl_item{cursor: pointer; border:1px solid #a7a7a7; background:#fff; color:#333; float:left; height:22px; line-height:22px; margin:0px 9px 10px 0px; text-align:center;padding:0px 10px; display:inline; overflow:hidden;}
.box { position: relative; background: #d9d9d9;overflow:hidden; color:#a4a4a4;}
.box .line {position: absolute;background: gray; top: 50%;left: -5px; right:-5px; height: 1px;}
.size_no{ border:1px dashed #c8c8c8; color:#c8c8c8;}  

/*面包屑下拉菜单*/
.tab{position:relative;display:block;float:left;cursor:pointer;line-height:25px;height:25px;color:#333;}
.tab a{color:#333;text-decoration:none}
.tab .tablink{position:relative;padding:0px 5px;display:inline-block;height:25px;line-height:25px;color:#333;z-index:99; border:1px solid #fff;}
.tab .up{padding-right:13px;background:url(img/xsearchico20120615.png) no-repeat #fff 32px -930px;}
.tab .down{padding-right:13px;background:url(img/xsearchico20120615.png) no-repeat #fff 36px -944px;}
.tab .tablink a{line-height:30px;color:#FFF;text-decoration:none}
.tab:hover{background-color:#FFF}
.tab:hover .tablink{color:#0060ae;border:1px solid #e37514;text-decoration:none}
.tab:hover .tablink a{color:#357d13}
.dropdown:hover .arwlink{border-bottom:1px solid #FFF;border-left:1px solid #e37514;height:24px}
.dropdown ul{display:none}
.dropdown:hover ul{position:absolute;display:block;margin-left:-1px;top:25px;left:1px;z-index:9; width:330px;}
.subnav{width:250px}
.nav .tab .subnav a{padding:4px 10px 4px 10px;line-height:16px;display:block;white-space:nowrap;color:#6c6c6c;float:left; background:url(img/xsearchico20120615.png) no-repeat 43px -961px; }
.subnav a:hover{text-decoration:underline;color:#e77119;}
.dropdown .subnav{background-color:#FFF;border:1px solid #e37514;padding:8px}
.size_no{ border:1px dashed #c8c8c8; color:#c8c8c8;}  
/**错误信息**/
.ferror_info {position: relative;height:350px;background: url(img/index/four_pic.gif) no-repeat 40%;}
.ferror_info p{position: absolute;width: 500px;left: 338px;top: 50%;}
.ferror_info p a {display: inline-block;width: 64px;height: 20px;line-height:20px;margin-right: 8px;text-align: center;font-weight: bold;color: rgb(255, 255, 255)!important;background: url(img/index/comm_img_sprite.gif) 0 -550px;}

/** 下拉菜单**/
.tabs{display: block;margin: 0;padding: 0;list-style: none;}
.tabs li{margin: 0;padding: 0;list-style: none;float: left;position: relative;}
.tabs a{position: relative; border:1px solid #fff; text-decoration:none; padding: 0px 16px 0px 5px;display: inline-block;max-width: 9.25em; text-overflow: ellipsis;white-space: nowrap;vertical-align: top;overflow: hidden;} 
.tabs .no_active_label{ font-size: 12px;font-weight: normal;padding: 0 5px;}
.tabs a.select{border: 1px solid #eb7410;border-bottom: none; height:28px;}
.tabs li.no_dropdown a{padding: 0px 5px;}
.tabs li i{ background:url(img/jts.jpg) no-repeat; display:inline-block;width:6px; height:13px;line-height:13px;vertical-align: top;overflow: hidden;margin: 7px 4px 0px 4px;}
.tabs li a{ color:#00579d;}
.tabs li.end a{ color:#333;}
.tabs .dropdown{margin: 0;display: block;position: absolute;z-index: 2;top: 28px;top: 28px\9;*top: 28px;width: 350px;visibility:hidden;left: 0; border:1px solid #eb7410; padding:10px 10px 10px 5px; background:#fff; line-height:20px;}
.tabs .dropdown li{margin: 0; padding:0px 3px 0px 5px; float: none;position: relative;list-style: none;display:inline-block; float:left;color:#6e6e6e;}
.tabs .dropdown li a{display:inline-block;text-align: center; color:#6e6e6e; }
.tabs .dropdown li a.selected{ color:#e96e11; font-weight:bold;}
.tabs .dropdown li a:hover{ color:#ee6e0d;}
.tabs .dropdown li a.selected {color: #E96E11;font-weight: bold;cursor:default;}
.tabs .on{z-index: 3;background:#fff;}
.tabs .on em{ background:url(img/xia.jpg) no-repeat; width:7px; height:4px; display:inline-block; vertical-align:middle; margin-left:3px;position: absolute;top: 12px;top: 10px\9;right: 6px;line-height: 4px;}

