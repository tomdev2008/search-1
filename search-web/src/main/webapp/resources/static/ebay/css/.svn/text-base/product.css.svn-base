.reco_con{ padding:12px; }
.reco_con li{ height:80px; padding-bottom:12px; margin-bottom:12px; border-bottom:1px dashed #b5b5b5;  }
.reco_con li.last{ border:none; padding-bottom:0px; margin-bottom:0px; }
.reco_con li .reco_gs,.reco_con li .gs_info,.reco_con li .param,.reco_con li .buyarea{ display:inline; float:left; height:80px; overflow:hidden; }
.reco_con li .reco_gs{ width:80px; }
.reco_con li .gs_info{ width:180px; margin:0 40px 0 12px; }
.reco_con li .param,.reco_con li .buyarea{ width:168px; }
.reco_con li .buyarea{ margin-left:40px; }
.reco_con li p{ padding-top:5px; }
.gs_info p a{ display:block; height:35px; line-height:17px; overflow:hidden; }
.gs_info p a:hover{ text-decoration:underline; }
.gs_info span{ display:block; margin-top:5px; font-weight:bold; font-size:14px; color:#d80c18; padding-left:9px; background:url(img/money_icon.gif) no-repeat 0 3px; }

.param p{ height:20px; line-height:20px; margin-bottom:12px; }
.param span,.buyarea span,.buyarea em,.param select,.buyarea input{ display:inline; float:left; }
.param select{ width:110px; border:1px solid #CCC; }
.buyarea em{ height:15px; padding-top:3px; padding-top:5px\0; *padding-top:5px; _padding-top:3px; }
.buyarea input{ width:50px; height:19px; line-height:19px; margin:0 6px; text-align:center; border:1px solid #c9c8ce; }


.buyarea span{ width:15px; height:19px; background:url(img/goods_info.gif) no-repeat -207px -160px; cursor:pointer; }
.buyarea span.up{ margin-right:3px; background-position:-207px -179px; }
.buyarea a{ display:block; width:73px; height:21px; margin:10px 0 0 48px; background:url(img/goods_info.gif) no-repeat -121px -258px; }
.buyarea a.daoh{ background-position:-195px -258px; }/*到货通知*/
.buyarea span.down,.buyarea span.up{ width:15px; height:19px; background:url(img/goods_info.gif) no-repeat; display:inline; float:left; cursor:pointer; }
.buyarea span.down{ background-position:-207px -160px; }
.buyarea span.up{ margin-right:3px; background-position:-207px -179px; }
.buyarea span.down.dis{ cursor:default; background-position:-224px -160px}
.buyarea span.up.dis{ cursor:default; background-position:-224px -179px}

#historyBox .right_2_pro p.txt{height:101px}
#historyBox .right_2_pro p.jc{ display:none}
