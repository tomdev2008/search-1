package com.xiu.search.core.bo;


/**
 * 品牌结果页面
 * @author Lipsion
 *
 */
public class BrandBo extends SearchBo {
	private String brandName;
	
	private String brandNameCN;
	
	private String brandNameEN;
	
	private String story;
	
	private String storeImgUrl;
	
	private String bannerImageName2;
	
	public String getBrandNameCN() {
		return brandNameCN;
	}

	public void setBrandNameCN(String brandNameCN) {
		this.brandNameCN = brandNameCN;
	}

	public String getBrandNameEN() {
		return brandNameEN;
	}

	public void setBrandNameEN(String brandNameEN) {
		this.brandNameEN = brandNameEN;
	}

	public String getBrandName() {
		return brandName;
	}

	public void setBrandName(String brandName) {
		this.brandName = brandName;
	}

	public String getStory() {
		return story;
	}

	public void setStory(String story) {
		this.story = story;
	}

	public String getStoreImgUrl() {
		return storeImgUrl;
	}

	public void setStoreImgUrl(String storeImgUrl) {
		this.storeImgUrl = storeImgUrl;
	}

	public String getBannerImageName2() {
		return bannerImageName2;
	}

	public void setBannerImageName2(String bannerImageName2) {
		this.bannerImageName2 = bannerImageName2;
	}

	
	

}
